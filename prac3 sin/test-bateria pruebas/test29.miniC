#include <stdio.h>
//Comprobacion de recursividad

int factorial(int n, float b, boolean c){
    int res;
    if(n==2){
        res = 2;
    } else {
        res = factorial(n-1, 4, true);
    }
    return res;
}

int main(){
    int a=20;
    printf(factorial(a, 4, true));
